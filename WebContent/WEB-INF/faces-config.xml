<navigation-rule>
 	<from-view-id>/humanResources.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>employee</from-outcome>
   		<to-view-id>/editEmployee.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
  	<from-view-id>/editEmployee.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>updated</from-outcome>
   		<to-view-id>/humanResources.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
  	<from-view-id>/createEmployee.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>created</from-outcome>
   		<to-view-id>/humanResources.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
  	<from-view-id>/archivedTimesheet.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>edit</from-outcome>
   		<to-view-id>/editTimesheet.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
  	<from-view-id>/createAdminTimesheet.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>edit</from-outcome>
   		<to-view-id>/editTimesheetRow.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
  	<from-view-id>/createTimesheetRow.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>created</from-outcome>
   		<to-view-id>/createAdminTimesheet.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
  	<from-view-id>/editTimesheetRow.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>updated</from-outcome>
   		<to-view-id>/createAdminTimesheet.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
  	<from-view-id>/editTimesheet.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>updated</from-outcome>
   		<to-view-id>/archivedTimesheet.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/showWP.xhtml</from-view-id>
	<navigation-case>
    	<from-outcome>createWP</from-outcome>
        <to-view-id>/createNewWP.xhtml</to-view-id>
     </navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/createNewWP.xhtml</from-view-id>
	<navigation-case>
    	<from-outcome>wpDetails</from-outcome>
        <to-view-id>/showWP.xhtml</to-view-id>
     </navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/*</from-view-id>
  	<navigation-case>
   		<from-outcome>logout</from-outcome>
   		<to-view-id>/login.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/changePassword.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>changed</from-outcome>
   		<to-view-id>/login.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/*</from-view-id>
  	<navigation-case>
   		<from-outcome>home</from-outcome>
   		<to-view-id>/adminLanding.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/adminLanding.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>projectsLanding</from-outcome>
