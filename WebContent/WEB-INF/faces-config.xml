</navigation-rule>
<navigation-rule>
	<from-view-id>/adminLanding.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>timesheetLanding</from-outcome>
   		<to-view-id>/timesheetAdmin.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/adminLanding.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>wpLanding</from-outcome>
   		<to-view-id>/getWorkpackages.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/getWorkpackages.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>weeklyReport</from-outcome>
   		<to-view-id>/createWeeklyReport.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/*</from-view-id>
  	<navigation-case>
   		<from-outcome>hrLanding</from-outcome>
   		<to-view-id>/humanResources.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/timesheetAdmin.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>create</from-outcome>
   		<to-view-id>/createAdminTimesheet.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/timesheetAdmin.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>archive</from-outcome>
   		<to-view-id>/archivedTimesheet.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/archivedTimesheet.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>viewArchivedTimesheet</from-outcome>
   		<to-view-id>/viewArchivedTimesheet.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/humanResources.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>editP</from-outcome>
   		<to-view-id>/editPayLevel.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<navigation-rule>
	<from-view-id>/editPayLevel.xhtml</from-view-id>
  	<navigation-case>
   		<from-outcome>PLupdated</from-outcome>
   		<to-view-id>/humanResources.xhtml</to-view-id>
  	</navigation-case>
</navigation-rule>
<application>
	<resource-bundle>
   		<base-name>META-INF.messages</base-name>
   		<var>msgs</var>
  	</resource-bundle>
</application>
</faces-config>

